program star_wars_virtuel_queue;
var n, i, j: integer;
begin

    //getting the needed data
    readln(n);

    for i:=n downto 1 do
      begin
           for j:=i downto 2 do
             begin
                  write(0)
             end;
            for j:=i to n do
             begin
                  write(1)
             end;
            writeln();
      end;
end.

